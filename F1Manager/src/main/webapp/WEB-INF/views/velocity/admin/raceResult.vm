<html>
	<head>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1"/>
		<title>#springMessageText("**message.admin.race_result**" "**message.admin.race_result**")</title>
		#parse("./favicon.vm")
	</head>
<body>

#parse("./admin/includes/menu.vm")

<div class="container table-responsive">
	
	<table class="table table-striped table-hover">
		<thead>
		  <tr>
			<th>#springMessageText("**message.admin.race_result.result**" "**message.admin.race_result.result**")</th>
			<th>#springMessageText("**message.admin.race_result.race**" "**message.admin.race_result.race**")</th>
			<th>#springMessageText("**message.admin.race_result.qualification**" "**message.admin.race_result.qualification**")</th>
		  </tr>
		</thead>
		<tbody>
			#foreach($driver in $drivers)
			  <tr>
				<td>$velocityCount</td>
				<td>$driver[0]</td>
				<td>$driver[1]</td>
			  </tr>
			 #end
		 </tbody>
	</table>
	
</div>

	
</body>
</html>
