<html>
	<head>
		<title></title>
	</head>
<body>
uj team form<br>


    <form action="saveTeam" method="post">
		
		<input style="display: none;" type="text" name="id" value="$!team.id"></input>
    
     	#springBind("team.name")
    	name<input type="text" name="name" value="$!team.name"></input> $!existingTeam
		#foreach($error in $status.errorMessages)$error #end<br>
    	
    	#springBind("team.price")
    	price<input type="text" name="price" value="$!team.price"></input>
		#foreach($error in $status.errorMessages) $error #end<br>
    	
		#springBind("team.picture")
    	picture<input type="text" name="picture" value="$!team.picture"></input>
		#foreach($error in $status.errorMessages) $error #end<br>
    	
		<input type="submit" value="save"></input>
    </form>	

</body>
</html>
